<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:name="com.example.adminwareoffood.MyApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AdminWareOfFood"
        tools:targetApi="33">
        <activity
            android:name="com.example.adminwareoffood.CompletedDetailsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.adminwareoffood.OrderDetailsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.adminwareoffood.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.example.adminwareoffood.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.adminwareoffood.SplashScreenActivity"
            android:exported="false" />
        <activity
            android:name="com.example.adminwareoffood.SignUpActivity"
            android:exported="false" />
        <activity
            android:name="com.example.adminwareoffood.AddItemActivity"
            android:exported="false" />
        <activity
            android:name="com.example.adminwareoffood.AllItemActivity"
            android:exported="false" />
        <activity
            android:name="com.example.adminwareoffood.CreateUserActivity"
            android:exported="false" />
        <activity
            android:name="com.example.adminwareoffood.AdminProfileActivity"
            android:exported="false" />
        <activity
            android:name="com.example.adminwareoffood.PendingOrderActivity"
            android:exported="false" />
        <activity
            android:name="com.example.adminwareoffood.OutForDeliveryActivity"
            android:exported="false" />
    </application>

</manifest>